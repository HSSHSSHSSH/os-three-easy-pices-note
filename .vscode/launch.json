{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Debug process-run with args",
      "type": "debugpy",
      "request": "launch",
      "program": "${workspaceFolder}/cpu-intro/process-run.py",
      "args": [
        "-l",
        "1:0,4:100",
        "-S",
        "SWITCH_ON_END",
        "-c",
        "-p"
      ],
      "console": "integratedTerminal",
      "justMyCode": false
    },
    {
      "name": "C/C++ Runner: Debug Session",
      "type": "cppdbg",
      "request": "launch",
      "args": [],
      "stopAtEntry": false,
      "externalConsole": true,
      "cwd": "d:/code/my/Operating-systems-Three-easy-pieces/cpu-api",
      "program": "d:/code/my/Operating-systems-Three-easy-pieces/cpu-api/build/Debug/outDebug",
      "MIMode": "gdb",
      "miDebuggerPath": "gdb",
      "setupCommands": [
        {
          "description": "Enable pretty-printing for gdb",
          "text": "-enable-pretty-printing",
          "ignoreFailures": true
        }
      ]
    }
  ]
}